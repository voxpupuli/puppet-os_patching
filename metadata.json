{
  "name": "puppet-os_patching",
  "version": "2.0.1-rc0",
  "author": "Vox Pupuli",
  "summary": "Perform OS level patching through tasks and report on patch levels through facts",
  "license": "Apache-2.0",
  "source": "https://github.com/voxpupuli/puppet-os_patching",
